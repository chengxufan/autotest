<case id="institution_getallproduct">
	<context>
	</context>
	<step note="获取产品列表">
		<helper name="helper.thrift">
			<item name="call" function="institution_getallproduct">
				<ret type="ProductListReultStruct">
					<val name="products" type="list">products</val>
				</ret>
			</item>
		</helper>
		<return>
			<item name="products" type="list">products</item>
		</return>
		<assert>
			<item name="notnull" var="products"></item>
		</assert>
	</step>
</case>
