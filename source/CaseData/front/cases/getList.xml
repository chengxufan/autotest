<case id="product_getList">	<context>		<item name="page">2</item>		<item name="size">10</item>	</context>	<step note="调用产品列表接口">		<helper name="helper.request.post">			<item name="uri">product/getList</item>			<item name="params">				<param name="params[page]" format="page">%s</param>				<param name="params[size]" format="size">%s</param>			</item>		</helper>		<return>			<item name="status">status</item>		</return>		<assert>			<item name="equals" var="status" param="10000"></item>		</assert>	</step>	</case>