<case id="questionnaire">	<context>		<item name="customer_id">10024</item>		<item name="questionnaire_answers">{"q1":"A","q2":"E","q3":"C","q4":"C","q5":"D","q6":"C","q7":"D","q8":"D","q9":"C","q10":"D"}</item>		<item name="serial_number">0065401</item>		<item name="questionnaire_score">100</item>	</context>	<step note="调用评级接口">		<helper name="helper.request.post">			<item name="uri">customer/questionnaire</item>			<item name="params">				<param name="params[customer_id]" format="customer_id">%s</param>				<param name="params[questionnaire_answers]" format="questionnaire_answers">%s</param>				<param name="params[serial_number]" format="serial_number">%s</param>				<param name="params[questionnaire_score]" format="questionnaire_score">%s</param>			</item>		</helper>		<return>			<item name="status">status</item>		</return>		<assert>			<item name="equals" var="status" param="10000"></item>		</assert>	</step>	</case>