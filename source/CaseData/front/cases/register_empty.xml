<case id="注册失败-必填项为空">	<context>		<item name="username"></item>		<item name="password"></item>		<item name="phonenumber"></item>		<item name="type"></item>	</context>	<step note="调用注册接口">		<helper name="helper.request.post">			<item name="uri">customer/register</item>			<item name="params">				<param name="params[username]" format="username">%s</param>				<param name="params[password]" format="password">%s</param>				<param name="params[phonenumber]" format="phonenumber">%s</param>				<param name="params[type]" format="type">%s</param>			</item>		</helper>		<return>			<item name="status">status</item>		</return>		<assert>			<item name="equals" var="status" param="10005"></item>		</assert>	</step>	</case>