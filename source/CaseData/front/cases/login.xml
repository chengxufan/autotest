<case id="注册">	<context>		<item name="username">45sttj00</item>		<item name="password">123456</item>		<item name="phonenumber">1324567888</item>		<item name="type">1</item>	</context>	<step note="调用注册接口">		<helper name="helper.request.post">			<item name="uri">customer/register</item>			<item name="params">				<param name="params[username]" format="username">%s</param>				<param name="params[password]" format="password">%s</param>				<param name="params[phonenumber]" format="phonenumber">%s</param>				<param name="params[type]" format="type">%s</param>			</item>		</helper>		<return>			<item name="serial_number">data.serial_number</item>		</return>		<assert>			<item name="notnull" var="serial_number"></item>		</assert>	</step>	<step note="检查数据库">		<helper name="helper.mysql.execute">			<item name="sql" format="phonenumber" statement="select"><![CDATA[SELECT COUNT(*) as c FROM tb_customer WHERE phonenumber ='%s']]></item>		</helper>		<return>			<item name="count">c</item>		</return>		<assert>			<item name="equals" var="count" param="1"></item>		</assert>	</step></case>